{
  "name": "@tzzack/g6-extension-vue",
  "version": "0.0.4",
  "type": "module",
  "description": "Using Vue Component to Define Your G6 Graph Node",
  "keywords": [
    "antv",
    "g6",
    "extension",
    "vue",
    "node"
  ],
  "repository": "https://github.com/TZZack/g6-extension-vue.git",
  "license": "MIT",
  "author": "TZZack",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist",
    "esm",
    "lib",
    "README",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p build:*",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib -p tsconfig.node.json",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir esm -p tsconfig.node.json",
    "build:umd": "rimraf ./dist && rollup -c",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue-demi": "^0.14.10"
  },
  "devDependencies": {
    "@antv/g": "^6.1.28",
    "@antv/g6": "^5.0.49",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.2.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vue/tsconfig": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "rollup": "^4.46.2",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "tslib": "^2.8.1",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vue": "^3.5.17",
    "vue-tsc": "^2.2.12"
  },
  "peerDependencies": {
    "@antv/g6": ">=5.0.0",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
